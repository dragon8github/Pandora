import mapbox from './mapbox.vue'

const mapboxConstructor = Vue.extend(mapbox);

let _initInstance;

const initInstance = () => {
  _initInstance = new mapboxConstructor({
    el: document.createElement('div')
  });
  document.body.appendChild(_initInstance.$el);
