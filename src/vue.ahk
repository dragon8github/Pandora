                            $app.scrollTop(scrollDistance `% distance)
                        } else {
                            // 滚动
                            $app.scrollTop(scrollDistance + 1)
                        }

                        // 尽量保证性能
                        this.timer = window.requestAnimFrame(_start)
                    }

                    // 开始滚动
                    _start()

                    // 鼠标开关
                    $app.mouseover(e => {
                        stop = true
                    }).mouseleave(e => {
                        stop = false
                        _start()
                    })
                }
            }
        }
