        }

        @else {
            justify-content: $x;
        }
    }

    @if $y {
        @if $y==s or $y==start {
            align-items: flex-start;
        }

        @else if $y==c or $y==center {
            align-items: center;
        }

        @else if $y==e or $y==end {
            align-items: flex-end;
        }

        @else if $y==stretch or $y==full or $y==f {
