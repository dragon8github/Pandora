    }

    .point:after {
        display: block;
        position: absolute;
        content: "";
        width: 80px;
        height: 80px;
        border-radius: 50`%;
        background: #3cb371;
        top: calc(50`% - 40px);
        left: calc(50`% - 40px);
        transform: scale(0);
        will-change: transform, opacity;
        opacity: 0
    }

    .point.changing:after {
        animation: emph 1.25s;
    }

    @keyframes emph {
        20`% {
            transform: none;
            opacity: .5
        }

        to {
            opacity: 0;
            transform: scale(1.2)
        }
    }
    </style>
